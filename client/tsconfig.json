{
    "compilerOptions": {
        "lib": ["dom", "dom.iterable"],
        "target": "es5",
        "noImplicitAny": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "es6",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": false
    }
}
